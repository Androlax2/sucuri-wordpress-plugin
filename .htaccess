
<IfModule !mod_authz_core.c>
	Order allow,deny
	Deny from all

	<FilesMatch "\.(gif|jpe?g|png|css|js)$">
	Order Allow,Deny
	Allow from all
	</FilesMatch>
</IfModule>

<IfModule mod_authz_core.c>
	Require all denied

	<FilesMatch "\.(gif|jpe?g|png|css|js)$">
	Require all granted
	</FilesMatch>
</IfModule>
